{
  "env": {
      "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "google"],
  "parserOptions": {
      "ecmaFeatures": {
          "experimentalObjectRestSpread": true,
          "jsx": true
      },
      "sourceType": "module"
  },
  "rules": {
    "quote-props": ["error", "as-needed"]
  }
}